{
  "name": "hannoverjs.de",
  "version": "2.0.0",
  "description": "The official Hannover.js Website",
  "main": "index.js",
  "private": true,
  "scripts": {
    "clean": "rimraf dist",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "test": "npm run lint",
    "start": "node -r babel-register ./node_modules/.bin/webpack-dev-server --config webpack.config.dev.js -d --history-api-fallback --hot --inline --progress --colors",
    "build": "npm run clean && cross-env NODE_ENV=production node -r babel-register ./node_modules/.bin/webpack --config webpack.config.prod.js --progress --colors",
    "deploy": "npm run build && node -r babel-register ./scripts/prepareDeploy.js && gh-pages -d dist -t -m 'deploy new version'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HannoverJS/hannoverjs.de.git"
  },
  "author": "HannoverJS",
  "bugs": {
    "url": "https://github.com/HannoverJS/hannoverjs.de/issues"
  },
  "homepage": "https://github.com/HannoverJS/hannoverjs.de#readme",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "axios": "^0.11.1",
    "babel-core": "^6.9.0",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-preset-stage-1": "^6.5.0",
    "cross-env": "^1.0.8",
    "css-loader": "^0.23.1",
    "eslint": "^2.11.0",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.8.1",
    "eslint-plugin-jsx-a11y": "^1.2.2",
    "eslint-plugin-react": "^5.1.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "fs-extra": "^0.30.0",
    "gh-pages": "^0.11.0",
    "humps": "^1.1.0",
    "json-loader": "^0.5.4",
    "postcss-custom-properties": "^5.0.1",
    "postcss-import": "^8.1.2",
    "postcss-loader": "^0.9.1",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-hot-loader": "^1.3.0",
    "react-icons": "^2.1.0",
    "react-markdown": "^2.2.0",
    "react-typewriter": "^0.4.1",
    "rimraf": "^2.5.2",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-hot-middleware": "^2.10.0"
  }
}
