{
  "name": "hannoverjs.de",
  "version": "2.0.0",
  "description": "The official Hannover.js Website",
  "private": true,
  "scripts": {
    "clean": "rm -rf build",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "test": "npm run lint",
    "start": "babel-node ./node_modules/.bin/webpack-dev-server --config webpack/dev.js -d --history-api-fallback --hot --inline --progress --colors",
    "build": "npm run clean && babel-node ./node_modules/.bin/webpack --config webpack/prod.js --progress --colors && cp CNAME build",
    "deploy": "npm run build && gh-pages -d build -t -m 'deploy new version'"
  },
  "repository": "hannoverjs/hannoverjs.de",
  "author": "Tim Cheung <tim@cheung.io>",
  "homepage": "https://hannoverjs.de",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.8",
    "babel-plugin-transform-class-properties": "^6.19.0",
    "babel-plugin-transform-object-rest-spread": "^6.19.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "css-loader": "^0.26.1",
    "eslint": "^3.11.1",
    "eslint-config-timche": "^2.0.1",
    "favicons-webpack-plugin": "^0.0.7",
    "file-loader": "^0.9.0",
    "gh-pages": "^0.12.0",
    "html-webpack-plugin": "^2.24.1",
    "html-webpack-template": "^5.4.2",
    "json-loader": "^0.5.4",
    "postcss-custom-properties": "^5.0.1",
    "postcss-import": "^9.0.0",
    "postcss-import-url": "^2.2.0",
    "postcss-loader": "^1.1.1",
    "react": "^15.4.0",
    "react-dom": "^15.4.0",
    "react-hot-loader": "^1.3.0",
    "react-icons": "^2.1.0",
    "react-markdown": "^2.2.0",
    "react-typewriter": "^0.4.1",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1",
    "whatwg-fetch": "^2.0.1"
  },
  "eslintConfig": {
    "extends": "timche"
  },
  "babel": {
    "presets" : [
      "es2015",
      "react"
    ],
    "plugins": [
      "transform-class-properties",
      "transform-object-rest-spread"
    ]
  }
}
